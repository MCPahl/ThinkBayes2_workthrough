    "#Set model for monster that attacked, assume each monster is equally likely in prior\n",
    "hypos = [6,8, 10]\n",
    "prior = Pmf(1, hypos)\n",
    "likelihood = 1/prior.qs\n",
    "posterior = prior * likelihood\n",
    "posterior.normalize()\n",
    "posterior\n",
    "#most likely a sword"
   ]
